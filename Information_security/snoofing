from scapy.all import sendp, Ether, IP, TCP, Raw

# Replace this with your real interface name (from get_if_list)
iface = "Wi-Fi"   # or "Local Area Connection* 10"

# Real IPv4 addresses (adjust these based on your LAN setup)
source_ip = "10.25.7.112"  # Your real IP
target_ip = "10.25.12.51" # Another device on your network

# Create an Ethernet frame with IP/TCP payload
packet = Ether() / IP(src=source_ip, dst=target_ip) / TCP(dport=80, flags="S") / Raw(load="Hi musky!")

# Send the packet
sendp(packet, iface=iface, verbose=True)
print("Packet sent.")